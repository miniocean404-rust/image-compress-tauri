{
  "build": {
    "beforeDevCommand": "pnpm vite:dev",
    "beforeBuildCommand": "pnpm vite:build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "image-compress-tauri",
    "version": "0.0.7"
  },
  "tauri": {
    "allowlist": {
      "fs": {
        "scope": [
          "*",
          "$DESKTOP/*"
        ],
        "all": true
      },
      "clipboard": {
        "all": true
      },
      "notification": {
        "all": true
      },
      "globalShortcut": {
        "all": true
      },
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "startDragging": true
      },
      "os": {
        "all": true
      }
    },
    "updater": {
      "active": true,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZFMTNGMDUyNTU3RTUyNTEKUldSUlVuNVZVdkFUL2xKMkxrSm4wY3Q4RVpJZ2tWOGlvSm44TC9ZdnF6aHI4emNPeE5HY1JvL3IK",
      "endpoints": [
        "https://miniocean404-rust.github.io/image-compress-tauri/install.json"
      ],
      "windows": {
        "installMode": "basicUi",
        "installerArgs": []
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.miniocean.compress.image",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "图片压缩",
        "center": true,
        "fullscreen": false,
        "fileDropEnabled": true,
        "resizable": true,
        "width": 800,
        "height": 600,
        "visible": true,
        "decorations": true,
        "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36",
        "hiddenTitle": true,
        "titleBarStyle": "Overlay"
      }
    ],
    "systemTray": {
      "iconPath": "icons/32x32.png",
      "title": "图片压缩",
      "iconAsTemplate": true
    }
  }
}