{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "image-compress-tauri",
    "version": "0.0.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "updater": {
      "active": true,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZFMTNGMDUyNTU3RTUyNTEKUldSUlVuNVZVdkFUL2xKMkxrSm4wY3Q4RVpJZ2tWOGlvSm44TC9ZdnF6aHI4emNPeE5HY1JvL3IK",
      "endpoints": [
        "https://miniocean404-rust.github.io/image-compress-tauri/install.json"
      ],
      "windows": {
        "installMode": "basicUi",
        "installerArgs": []
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.miniocean.compress.image",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "center": true,
        "width": 400,
        "height": 300,
        "decorations": false,
        "url": "./splash.html",
        "label": "splash"
      },
      {
        "title": "图片压缩",
        "center": true,
        "fullscreen": false,
        "fileDropEnabled": true,
        "resizable": true,
        "width": 800,
        "height": 600,
        "visible": false
      }
    ]
  }
}
